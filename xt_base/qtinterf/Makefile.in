#######################################################################
LOCATION = xt_base/qtinterf
#######################################################################

DATE = @DATE@

CXX = @CXX@
CC = @CC@
CFLAGS = @CFLAGSG@ @TOOLKITCFLAGS@ @UFLAGS@
DEPEND_PROG = @DEPEND_PROG@ @CFLAGSG@ @TOOLKITCFLAGS@
AR = @AR@
RANLIB = @RANLIB@
FILTER = @FILTER@
MOC = @MOC@

INCLUDE = -I../include -I../include/qtinterf -I../../mozy/include \
  @TOOLKITINC@
MOC_INCLUDE = ../include/qtinterf

LIB_TARGET = ../lib/qtinterf.a

CCFILES = \
  activity_w.cc draw_if.cc draw_gl_w.cc draw_qt_w.cc draw_x_w.cc \
  idle_proc.cc interval_timer.cc polysplit.cc progress_d.cc \
  qtaffirm.cc qtedit.cc qtfile.cc qtfont.cc qthcopy.cc qtinput.cc \
  qtinterf.cc qtlist.cc qtmcol.cc qtmsg.cc qtnumer.cc search_d.cc \

MOCFILES = \
    moc_activity_w.cpp \
    moc_affirm.cpp \
    moc_draw_gl_w.cpp \
    moc_draw_qt_w.cpp \
    moc_draw_x_w.cpp \
    moc_file.cpp \
    moc_font.cpp \
    moc_idle_proc.cpp \
    moc_input.cpp \
    moc_interval_timer.cpp \
    moc_list.cpp \
    moc_mcol.cpp \
    moc_msg.cpp \
    moc_numer.cpp \
    moc_hcopy.cpp \
    moc_progress_d.cpp \
    moc_search_d.cpp \
    moc_edit.cpp

CCOBJS = $(CCFILES:.cc=.o)
MOCOBJS = $(MOCFILES:.cpp=.o)
LIBOBJS = $(CCOBJS) $(MOCOBJS)

$(LIB_TARGET): $(LIBOBJS)
	-@rm -f $(LIB_TARGET); \
	$(AR) cr $(LIB_TARGET) $(LIBOBJS)
	$(RANLIB) $(LIB_TARGET)

.cpp.o:
	$(CXX) -c $(CFLAGS) $(INCLUDE) -o $@ $<

.cc.o:
	$(CXX) -c $(CFLAGS) $(INCLUDE) -o $@ $<

moc_activity_w.o: moc_activity_w.cpp  $(MOC_INCLUDE)/activity_w.h 
moc_affirm.o: moc_affirm.cpp  $(MOC_INCLUDE)/qtaffirm.h 
moc_draw_gl_w.o: moc_draw_gl_w.cpp  $(MOC_INCLUDE)/draw_gl_w.h 
moc_draw_qt_w.o: moc_draw_qt_w.cpp  $(MOC_INCLUDE)/draw_qt_w.h 
moc_draw_x_w.o: moc_draw_x_w.cpp  $(MOC_INCLUDE)/draw_x_w.h 
moc_file.o: moc_file.cpp  $(MOC_INCLUDE)/qtfile.h 
moc_font.o: moc_font.cpp  $(MOC_INCLUDE)/qtfont.h 
moc_idle_proc.o: moc_idle_proc.cpp  $(MOC_INCLUDE)/idle_proc.h 
moc_input.o: moc_input.cpp  $(MOC_INCLUDE)/qtinput.h 
moc_interval_timer.o: moc_interval_timer.cpp  $(MOC_INCLUDE)/interval_timer.h 
moc_list.o: moc_list.cpp  $(MOC_INCLUDE)/qtlist.h
moc_mcol.o: moc_mcol.cpp  $(MOC_INCLUDE)/qtmcol.h
moc_msg.o: moc_msg.cpp  $(MOC_INCLUDE)/qtmsg.h 
moc_numer.o: moc_numer.cpp  $(MOC_INCLUDE)/qtnumer.h 
moc_hcopy.o: moc_hcopy.cpp  $(MOC_INCLUDE)/qthcopy.h 
moc_progress_d.o: moc_progress_d.cpp  $(MOC_INCLUDE)/progress_d.h 
moc_search_d.o: moc_search_d.cpp  $(MOC_INCLUDE)/search_d.h 
moc_edit.o: moc_edit.cpp  $(MOC_INCLUDE)/qtedit.h 

moc_activity_w.cpp: $(MOC) $(MOC_INCLUDE)/activity_w.h
	$(MOC) $(MOC_INCLUDE)/activity_w.h -o moc_activity_w.cpp
moc_affirm.cpp: $(MOC) $(MOC_INCLUDE)/qtaffirm.h
	$(MOC) $(MOC_INCLUDE)/qtaffirm.h -o moc_affirm.cpp
moc_draw_gl_w.cpp: $(MOC) $(MOC_INCLUDE)/draw_gl_w.h
	$(MOC) $(MOC_INCLUDE)/draw_gl_w.h -o moc_draw_gl_w.cpp
moc_draw_qt_w.cpp: $(MOC) $(MOC_INCLUDE)/draw_qt_w.h
	$(MOC) $(MOC_INCLUDE)/draw_qt_w.h -o moc_draw_qt_w.cpp
moc_draw_x_w.cpp: $(MOC) $(MOC_INCLUDE)/draw_x_w.h
	$(MOC) $(MOC_INCLUDE)/draw_x_w.h -o moc_draw_x_w.cpp
moc_file.cpp: $(MOC) $(MOC_INCLUDE)/qtfile.h
	$(MOC) $(MOC_INCLUDE)/qtfile.h -o moc_file.cpp
moc_font.cpp: $(MOC) $(MOC_INCLUDE)/qtfont.h
	$(MOC) $(MOC_INCLUDE)/qtfont.h -o moc_font.cpp
moc_idle_proc.cpp: $(MOC) $(MOC_INCLUDE)/idle_proc.h
	$(MOC) $(MOC_INCLUDE)/idle_proc.h -o moc_idle_proc.cpp
moc_input.cpp: $(MOC) $(MOC_INCLUDE)/qtinput.h
	$(MOC) $(MOC_INCLUDE)/qtinput.h -o moc_input.cpp
moc_interval_timer.cpp: $(MOC) $(MOC_INCLUDE)/interval_timer.h
	$(MOC) $(MOC_INCLUDE)/interval_timer.h -o moc_interval_timer.cpp
moc_list.cpp: $(MOC) $(MOC_INCLUDE)/qtlist.h
	$(MOC) $(MOC_INCLUDE)/qtlist.h -o moc_list.cpp
moc_mcol.cpp: $(MOC) $(MOC_INCLUDE)/qtmcol.h
	$(MOC) $(MOC_INCLUDE)/qtmcol.h -o moc_mcol.cpp
moc_msg.cpp: $(MOC) $(MOC_INCLUDE)/qtmsg.h
	$(MOC) $(MOC_INCLUDE)/qtmsg.h -o moc_msg.cpp
moc_numer.cpp: $(MOC) $(MOC_INCLUDE)/qtnumer.h
	$(MOC) $(MOC_INCLUDE)/qtnumer.h -o moc_numer.cpp
moc_hcopy.cpp: $(MOC) $(MOC_INCLUDE)/qthcopy.h
	$(MOC) $(MOC_INCLUDE)/qthcopy.h -o moc_hcopy.cpp
moc_progress_d.cpp: $(MOC) $(MOC_INCLUDE)/progress_d.h
	$(MOC) $(MOC_INCLUDE)/progress_d.h -o moc_progress_d.cpp
moc_search_d.cpp: $(MOC) $(MOC_INCLUDE)/search_d.h
	$(MOC) $(MOC_INCLUDE)/search_d.h -o moc_search_d.cpp
moc_edit.cpp: $(MOC) $(MOC_INCLUDE)/qtedit.h
	$(MOC) $(MOC_INCLUDE)/qtedit.h -o moc_edit.cpp

mocclean:
	-@rm -f $(MOCOBJS)
	-@rm -f $(MOCFILES)

clean: mocclean
	-@rm -f $(CCOBJS) $(LIB_TARGET)

distclean: clean
	-@rm -f Makefile

depend:
	@echo depending in qtinterf
	@if [ x$(DEPEND_DONE) = x ]; then \
	    echo DEPEND_DONE = 1 >> Makefile; \
	    $(DEPEND_PROG) $(INCLUDE) $(CCFILES) $(FILTER) >> Makefile; \
	fi

install:

uninstall:

#######################################################################
