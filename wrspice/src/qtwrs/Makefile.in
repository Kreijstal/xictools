SPICE_VERSION = @SPICE_VERSION@
DATE = @DATE@

CXX = @CXX@
CC = @CC@
CFLAGS = @CFLAGSG@ @NEEDINT64@ @TOOLKITCFLAGS@ @UFLAGS@ -DWRSPICE
DEPEND_PROG = @DEPEND_PROG@ @CFLAGSG@ @NEEDINT64@ @TOOLKITCFLAGS@ -DWRSPICE
WINDRES = @WINDRES@
AR = @AR@
LD = @LD@
RANLIB = @RANLIB@
FILTER = @FILTER@
USEMOZY = @USEMOZY@
TOOLKITINC = @TOOLKITINC@
RESOURCE = @RESOURCE@
MOC = @MOC@

LIB_TARGET = qtwrs.a

BASE = ../../../xt_base
SECURE = ../../../secure
INCLUDE = -I. -I../../include -I$(BASE)/include -I$(SECURE)/include

CCFILES = \
    qterrmsg.cc \
    qtgraph.cc \
    qtplot.cc \
    qtspmsg.cc \
    qttbdlg.cc \
    qttbhelp.cc \
    qttoolb.cc
#    qtcmds.cc \
#    qtcolor.cc \
#    qtdebug.cc \
#    qtfte.cc \
#    qtpldef.cc \
#    qtshell.cc \
#    qtsim.cc \

MOCFILES = \
    moc_errmsg.cpp \
    moc_plot.cpp \
    moc_spmsg.cpp \
    moc_tbdlg.cpp \
    moc_tbhelp.cpp
#    moc_cmds.cpp \
#    moc_color.cpp \
#    moc_debug.cpp \
#    moc_fte.cpp \
#    moc_pldef.cpp \
#    moc_plot.cpp \
#    moc_shell.cpp \
#    moc_sim.cpp \

CCOBJS = $(CCFILES:.cc=.o) $(RESOURCE)
MOCOBJS = $(MOCFILES:.cpp=.o)
LIBOBJS = $(CCOBJS) $(MOCOBJS)

$(LIB_TARGET): $(LIBOBJS)
	@if [ -f $(LIB_TARGET) ]; then \
	    rm -f $(LIB_TARGET); \
	fi
	$(AR) cr $(LIB_TARGET) $(LIBOBJS)
	$(RANLIB) $(LIB_TARGET)

ifeq ($(USEMOZY), yes)
MOZY = ../../../mozy
MOZYINC = -I$(MOZY)/include
MOBJS = qtmain.o

$(MOBJS):
	$(CXX) $(CFLAGS) $(INCLUDE) $(MOZYINC) $(TOOLKITINC) -c $*.cc
endif

.cpp.o:
	$(CXX) -c $(CFLAGS) $(INCLUDE) $(TOOLKITINC) -o $@ $<

.cc.o:
	$(CXX) $(CFLAGS) $(INCLUDE) $(TOOLKITINC) -c $*.cc

